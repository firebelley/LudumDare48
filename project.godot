[autoload]

GameState="*res://scripts/State/GameState.cs"
LevelManager="*res://scripts/LevelManager.cs"
TransitionManager="*res://scenes/TransitionManager.tscn"
Cursor="*res://scenes/UI/Cursor.tscn"
ButtonClicker="*res://scenes/ButtonClicker.tscn"
